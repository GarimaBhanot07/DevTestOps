pipeline{
    agent any
    tools{
        maven 'Maven'
    }

    stages{
    stage ('checkout')
		{
			steps
			{
				checkout scm
			}
		}
		stage ('Build')
		{
			steps
			{
				bat "mvn install"
			}
		}
		stage ('Clean and Test')
		{
			steps
			{
				bat "mvn clean test"
			}
		}
		
    }
    post{
        success{
            bat "echo success"
        }
    }

}
